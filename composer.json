{
  "name": "powerkernel/yii-app",
  "description": "Yii App Template from powerkernel",
  "keywords": [
    "yii2",
    "framework",
    "project template"
  ],
  "homepage": "https://github.com/powerkernel/yii-app",
  "type": "project",
  "license": "MIT License",
  "support": {
    "issues": "https://github.com/powerkernel/yii-app/issues",
    "wiki": "https://github.com/powerkernel/yii-app/wiki",
    "source": "https://github.com/powerkernel/yii-app"
  },
  "authors": [
    {
      "name": "Harry Tang",
      "email": "harry@powerkernel.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "2.1.x-dev",
    "yiisoft/yii2-debug": "2.1.x-dev",
    "yiisoft/yii2-gii": "2.1.x-dev",
    "yiisoft/yii2-swiftmailer": "2.2.x-dev",
    "yiisoft/yii2-bootstrap": "2.2.x-dev",
    "yiisoft/yii2-jui": "2.1.x-dev",
    "yiisoft/yii2-httpclient": "2.1.x-dev",
    "yiisoft/yii2-authclient": "2.2.x-dev",
    "yiisoft/yii2-mongodb": "2.2.x-dev",
    "yiisoft/yii2-imagine": "2.2.x-dev",
    "yiisoft/yii2-faker": "2.1.x-dev"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "2.1.x-dev",
    "yiisoft/yii2-gii": "2.1.x-dev",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "app\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  },
  "config": {
    "process-timeout": 1800
  }
}